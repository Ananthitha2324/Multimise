N = int(input("Enter the limit:"))
primenumbers = []
for i in range(2, N+1):
    for j in range(2, i):
        if i%j == 0:
            break
    else:
        primenumbers.append(i)
print("the prime numbers are", primenumbers)        
print(len(primenumbers), "prime numbers are within this limit")
            
    
